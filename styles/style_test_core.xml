<?xml version="1.0"?>

<!-- An ultra simple Mapnik stylesheet -->

<!DOCTYPE Map [
<!ENTITY google_mercator "+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +wktext +over +no_defs">
]>

<Map srs="&google_mercator;" background-color="black">
    <Style name="style">
        <Rule>
            <PolygonSymbolizer fill="white"/>
        </Rule>
    </Style>
    <Layer name="layer" srs="&google_mercator;">
        <StyleName>style</StyleName>
        <Datasource>
            <Parameter name="type">shape</Parameter>
            <Parameter name="file">/tiles/shapes/grid_merc2.shp</Parameter>
            <LineSymbolizer stroke="#0000ff" stroke-width="4" />
        </Datasource>
    </Layer>
</Map>